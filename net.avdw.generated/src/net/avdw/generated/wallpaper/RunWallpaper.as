			var to:DisplayObject = wallpaper.create(stage.stageWidth>>1, stage.stageHeight>>1);
			transition.transition(from, to);
			
			title.text = wallpaper + "";
			title.y = (sprite.height - title.textHeight) >> 1;
			title.x = (sprite.width - title.textWidth) >> 1;
			
			sprite.addChild(title);
		}
