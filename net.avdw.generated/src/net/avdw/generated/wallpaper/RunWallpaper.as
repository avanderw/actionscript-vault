		private function startup(e:Event):void
		{
			removeEventListener(Event.ENTER_FRAME, startup);
			
			var textFormat:TextFormat = new TextFormat(null, 16, 0x0, true, false, false, null, null, TextFormatAlign.LEFT);
			titleTL.defaultTextFormat = textFormat;
